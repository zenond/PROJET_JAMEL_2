function tstId(i){
	var color;
	$("#"+labelArray[i].id).on('click' , function(){
		color= d3.select(pathFiltering[i]);
		color.style("opacity" , 0);	
		alert("clicked id bricol√© dakhel mbeddel w filtre");
			
		});


}

function tstCheck(){
		var myDiv = document.getElementById("graphic");
			var checkbox;
			var text; 
			for (var i = 0; i < 2; i++) {
				var label = document.createElement("label");
				checkbox = document.createElement("input");
				text = document.createTextNode("check test"+i);

				checkbox.setAttribute("type", "checkbox");
				checkbox.setAttribute("value" , true);
				
				label.setAttribute("class" , "9 checkbox-inline btn-default active");
				label.setAttribute("title" , "kach value");
				label.setAttribute("id" , "l"+i);

				label.appendChild(text);
				label.appendChild(checkbox);
				myDiv.appendChild(label);
				labelArray.push(label); 
				checkbox.checked = true; 
				console.log(labelArray[i].id);
			}

			tstId(0);
			tstId(1);
}